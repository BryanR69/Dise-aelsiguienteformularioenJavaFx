<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" spacing="10" alignment="TOP_LEFT" fx:controller="com.example.javafxcontrols.YourController">
    <children>
        <HBox spacing="10" alignment="CENTER_LEFT">
            <children>
                <Label text="Button:"/>
                <Button text="Button"/>
            </children>
        </HBox>
        <HBox spacing="10" alignment="CENTER_LEFT">
            <children>
                <Label text="CheckBox:"/>
                <CheckBox text="CheckBox"/>
            </children>
        </HBox>
        <HBox spacing="10" alignment="CENTER_LEFT">
            <children>
                <Label text="Hyperlink:"/>
                <Hyperlink text="Hyperlink"/>
            </children>
        </HBox>
        <HBox spacing="10" alignment="CENTER_LEFT">
            <children>
                <Label text="ToggleButton:"/>
                <ToggleButton text="ToggleButton"/>
            </children>
        </HBox>
        <HBox spacing="10" alignment="CENTER_LEFT">
            <children>
                <Label text="RadioButton:"/>
                <RadioButton text="RadioButton"/>
            </children>
        </HBox>
        <HBox spacing="10" alignment="CENTER_LEFT">
            <children>
                <Label text="Label:"/>
                <Label text="Label"/>
            </children>
        </HBox>
        <HBox spacing="10" alignment="CENTER_LEFT">
            <children>
                <Label text="TextField:"/>
                <TextField text="some text..."/>
            </children>
        </HBox>
        <HBox spacing="10" alignment="CENTER_LEFT">
            <children>
                <Label text="PasswordField:"/>
                <PasswordField text="password"/>
            </children>
        </HBox>
        <HBox spacing="10" alignment="CENTER_LEFT">
            <children>
                <Label text="TextArea:"/>
                <TextArea text="This is very long text that will wrap to several lines."/>
            </children>
        </HBox>
        <HBox spacing="10" alignment="CENTER_LEFT">
            <children>
                <Label text="ProgressIndicator:"/>
                <ProgressIndicator progress="0.49"/>
                <Label text="49%"/>
            </children>
        </HBox>
        <HBox spacing="10" alignment="CENTER_LEFT">
            <children>
                <Label text="ProgressBar:"/>
                <ProgressBar progress="0.49"/>
            </children>
        </HBox>
        <HBox spacing="10" alignment="CENTER_LEFT">
            <children>
                <Label text="Slider:"/>
                <Slider/>
            </children>
        </HBox>
    </children>
</VBox>
